                    }
                    else if (etype === 'year') {
                        if ((!isNaN(chVal) || isNaN(chNum)) && ((chNum === 9) || (chNum === 99))) {
                            document.forms[FNAME].elements[arg].value = "1999";
                            chNum = 1999
                        }
                        if ((!isNaN(chVal) || isNaN(chNum)) && ((chVal === "0") || (chVal === "00"))) {
                            document.forms[FNAME].elements[arg].value = "2000";
