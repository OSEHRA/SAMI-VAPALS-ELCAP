                function fudCalc(mode) {
                    var aMsg = "";
// vcode
                    var base = 'rbdmr';
                    var hvinc = 'hvcefuw';
                    var dest = 'cefud';

                    var baseValue = document.forms[FNAME].elements[base].value;
