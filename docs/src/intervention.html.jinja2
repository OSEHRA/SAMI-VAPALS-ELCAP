                    else if (incStr === "1m") {
                        minc = 1;
                        dinc = 0;
                        yinc = 0;
                    }
                    else if (incStr === "3m") {
                        minc = 3;
                        dinc = 0;
                        yinc = 0;
                    }
                    else if (incStr === "6m") {
                        minc = 6;
                        dinc = 0;
