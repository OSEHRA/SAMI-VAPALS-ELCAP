                function cdt(da, db, sense) {
                    var cV = [];
                    var mval = [];
                    var dval = [];
                    var yval = [];
                    var mstr, dstr, ystr;
                    var exp = /^(\d{1,2}\/\D{3}\/\d{4})$/;
                    var wrong = false;
                    var dn = -1;

                    cV[0] = document.forms[FNAME].elements[da].value;
                    cV[0] = cV[0].replace(/^\s+/g, '').replace(/\s+$/g, '');

                    cV[1] = document.forms[FNAME].elements[db].value;
                    cV[1] = cV[1].replace(/^\s+/g, '').replace(/\s+$/g, '');

                    if ((cV[0] === "") && (cV[1] === "")) {
                        return da;
                    }
                    else if (cV[0] === "") {
                        return db;
                    }
                    else if (cV[1] === "") {
                        return da;
                    }

                    for (var i = 0; i < 2; i++) {
                        if (!exp.test(cV[i])) {
                            wrong = true;
                        }
                        else {
                            var pd = new RegExp("\\/", "g");
                            var dfa = cV[i].split(pd);

                            if (dfa.length === 3) {
                                dstr = dfa[0];
                                if (dstr.length === 1) {
                                    dstr = "0" + dstr;
                                }
                                mstr = dfa[1];
                                if (dfa[1].length === 3) {
                                    mstr = fixMonth(dfa[1]);
                                }
                                else {
                                    mstr = dfa[1];
                                }

                                ystr = dfa[2];
                                if (ystr.length === 4)
                                    syear = false;
                                else
                                    wrong = true;

                                mval[i] = parseInt(mstr, 10);
                                dval[i] = parseInt(dstr, 10);
                                yval[i] = parseInt(ystr, 10);
                            }
                            else {
                                wrong = true;
                            }
                        }

                        if (wrong) {
                            mval[0] = 0;
                            dval[0] = 0;
                            yval[0] = 0;
                        }
                    }

                    if (yval[0] > yval[1])
                        dn = 0;
                    else if (yval[0] < yval[1])
                        dn = 1;
                    else if (mval[0] > mval[1])
                        dn = 0;
                    else if (mval[0] < mval[1])
                        dn = 1;
                    else if (dval[0] > dval[1])
                        dn = 0;
                    else if (dval[0] < dval[1])
                        dn = 1;
                    else {
                        dn = 0;
                    }

                    if (sense === 1) {
                        if (dn === 0)
                            return da;
                        else if (dn === 1)
                            return db;
                        else
                            alert("cdt error!");
                    }
                    else if (sense === -1) {
                        if (dn === 0)
                            return db;
                        else if (dn === 1)
                            return da;
                        else
                            alert("cdt error!");
                    }
                    else {
                        alert("cdt error!");
                    }
                }

